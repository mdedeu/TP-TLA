main {
    // Crear un árbol a partir de otro, que solo utilice los nodos cuyo nombre es “Maginer”
    tree string arbol;

    arbol.createNode("root", "Maginer");
    arbol.createNode("firstSon", "NO");
    arbol.createNode("secondSon", "NO");
    arbol.createNode("thirdSon", "Maginer");

    tree int newTree = arbol.filter("Maginer");
}