main {
    // Crear un árbol a partir de otro, que solo utilice los nodos cuyo nombre es “Maginer”
    binaryTree string arbol;

    arbol.createNode("Maginer");
    arbol.createNode("NO");
    arbol.createNode("NO");
    arbol.createNode("Maginer");

    binaryTree string newTree = arbol.filter("Maginer");
}