main {
    write("Programa para armar arbol de nombres");
    int integrantes;
    write("Escribe la cantidad de integrantes");
    read(integrantes);

    string nombre;
    avlTree string arbol;
    while(integrantes >= 0) {
        write("Escribe un nombre: ");
        read(nombre);
        arbol.createNode(nombre);
        integrantes = integrantes - 1;
    }

    arbol.print();

}