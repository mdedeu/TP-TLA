main {
    // Crear un árbol a partir de otro, que solo utilice los nodos cuyo nombre es “Maginer”
    rbtree string arbol;

    arbol.createNode("Maginer");
    arbol.createNode("NO");
    arbol.createNode("NO");
    arbol.createNode("Maginer");
    arbol.print();
    rbtree string newTree;
    newTree = arbol.filter("Maginer");
    newTree.print();
}