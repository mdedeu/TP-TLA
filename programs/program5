main {
    //Obtener un nuevo árbol multiplicando el número entero almacenado en sus nodos por 3
    bstTree int arbol;
    arbol.createNode(1);
    arbol.createNode(2);
    arbol.createNode(3);
    arbol.createNode(4);
    
    bstTree int arbolito;
    arbolito = arbol.mul(3);
    arbolito.print();
   
}